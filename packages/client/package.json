{
  "name": "@tritio/client",
  "version": "0.3.1",
  "description": "Type-safe client for Tritio",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target browser && tsc --emitDeclarationOnly --outDir dist"
  }
}
