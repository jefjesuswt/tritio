{
  "name": "@tritio/jwt",
  "version": "1.0.0",
  "description": "JWT authentication utilities for Tritio",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/jwt/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/jwt/src/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun && tsc --emitDeclarationOnly --outDir dist"
  },
  "peerDependencies": {
    "tritio": "workspace:*"
  },
  "dependencies": {
    "jose": "^6.1.3"
  },
  "devDependencies": {
    "tritio": "workspace:*",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
