{
  "name": "@tritio/testing",
  "version": "0.1.0",
  "description": "Testing utilities for Tritio",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "peerDependencies": {
    "tritio": "workspace:*"
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun && tsc --emitDeclarationOnly --outDir dist"
  },
  "devDependencies": {
    "tritio": "workspace:*",
    "typescript": "^5.0.0"
  }
}
