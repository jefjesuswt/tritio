{
  "name": "tritio",
  "version": "0.0.1",
  "description": "The fast, opinionated h3 framework",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
