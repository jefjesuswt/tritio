{
  "name": "tritio",
  "version": "0.3.1",
  "description": "The fast, opinionated h3 framework",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun && tsc --emitDeclarationOnly --outDir dist"
  },
  "dependencies": {
    "consola": "^3.4.2",
    "h3": "^2.0.1-rc.8",
    "picocolors": "^1.1.1",
    "typebox": "^1.0.78"
  },
  "devDependencies": {
    "@tritio/testing": "workspace:*",
    "@types/node": "^25.0.9"
  }
}
