  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun --sourcemap"
  },
  "dependencies": {
    "defu": "^6.1.4",
    "fast-json-stringify": "^6.1.1",
    "h3": "^2.0.1-rc.7",
    "typebox": "^1.0.77"
  },
  "publishConfig": {
    "access": "public"
  }
}